citHeader("To cite package 'geigen' in publications use:")

citEntry(entry="manual",
  title = "geigen: Calculate Generalized Eigenvalues of a Matrix Pair\n",
  year = "2015",
  author = personList(as.person("Berend Hasselman")),
  note = "R package 1.5",
  textVersion =  paste("Hasselman, B. (2015), geigen: Calculate Generalized Eigenvalues of a Matrix Pair.
  R-package version 1.3, URL http://CRAN.R-project.org/package=geigen"),
  url = {"http://CRAN.R-project.org/package=geigen"},
         )
